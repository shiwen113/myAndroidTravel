<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#32A6C0"
    tools:context="com.example.personage.PersonageActivity" >

    <RelativeLayout
        android:id="@+id/linearLayout1"
        android:layout_width="330dp"
        android:layout_height="179dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/yangguang"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/IM_MyloginActivity_personalinformation"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/shezhi" />

        <ImageView
           android:id="@+id/IM_MyloginActivity_loginpage"
            android:layout_width="60dp"
            android:layout_height="60dp"
            
            android:layout_below="@+id/IM_MyloginActivity_personalinformation"
            android:layout_centerHorizontal="true"
            android:src="@drawable/welcome_aboard" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="23dp"
            android:layout_height="23dp"
            android:layout_alignLeft="@+id/IM_MyloginActivity_loginpage"
            android:layout_below="@+id/IM_MyloginActivity_loginpage"
            android:src="@drawable/like" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView10"
            android:layout_alignRight="@+id/IM_MyloginActivity_loginpage"
            android:text="0000" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/tableRow1"
        android:layout_width="fill_parent"
        android:layout_height="100dp"
        android:gravity="center"
        android:layout_alignLeft="@+id/linearLayout1"
        android:layout_below="@+id/linearLayout1"
        android:background="#32A6C0" >

        <ImageView
            android:id="@+id/IM_MyloginActivity_PERSIONMESSAGEPICTURE"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/im_MyloginActivity_Myshare"
            android:src="@drawable/xin" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TV_MyloginActivity_persionmessage"
            android:layout_alignBottom="@+id/TV_MyloginActivity_persionmessage"
            android:layout_alignParentRight="true"
            android:onClick="click"
            android:text="我的分享" />

        <ImageView
            android:id="@+id/IM_MyloginActivity_Mytravelteam"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignLeft="@+id/textView2"
            android:layout_alignParentTop="true"
            android:layout_alignRight="@+id/textView2"
            android:src="@drawable/home" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/IM_MyloginActivity_Mytravelteam"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="14dp"
            android:onClick="click"
            android:text="我的旅行队" />

        <ImageView
            android:id="@+id/im_MyloginActivity_Myshare"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/IM_MyloginActivity_Mytravelteam"
            android:src="@drawable/fangxiang" />

        <TextView
            android:id="@+id/TV_MyloginActivity_persionmessage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView2"
            android:layout_alignBottom="@+id/textView2"
            android:layout_alignParentLeft="true"
            android:onClick="click"
            android:text="个人信息" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/tableRow2"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/tableRow1"
        android:layout_span="3"
        android:background="#32A6C0" >

        <ImageView
            android:id="@+id/IM_MyloginActivity_loginpicture"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:src="@drawable/twitter" />

        <Button
            android:id="@+id/BT_MyloginActivity_myregister"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/imageView4"
            android:layout_toRightOf="@+id/IM_MyloginActivity_loginpicture"
            android:background="#32A6C0"
            
            android:gravity="clip_vertical"
            android:text="" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/IM_MyloginActivity_loginpicture"
            android:layout_alignLeft="@+id/BT_MyloginActivity_myregister"
            android:layout_marginLeft="21dp"
            android:text="注册" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="23dp"
            android:layout_height="23dp"
            android:layout_alignBottom="@+id/textView1"
            android:layout_alignParentRight="true"
            android:src="@drawable/toright" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/tableRow2"
        android:layout_below="@+id/tableRow2"
        android:background="#32A6C0"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/IM_MyloginActivity_mytravelteammessage"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:src="@drawable/lens" />

        <Button
            android:id="@+id/BT_MyloginActivity_myteammessage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/imageView6"
            android:layout_toRightOf="@+id/IM_MyloginActivity_mytravelteammessage"
            android:background="#32A6C0"
            android:text="" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="23dp"
            android:layout_height="23dp"
            android:layout_alignBottom="@+id/IM_MyloginActivity_mytravelteammessage"
            android:layout_alignParentRight="true"
            android:src="@drawable/toright" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/BT_MyloginActivity_myteammessage"
            android:layout_centerVertical="true"
            android:layout_marginLeft="18dp"
            android:text="旅行队消息" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:background="#32A6C0"
        android:layout_below="@+id/relativeLayout1" >

        <ImageView
            android:id="@+id/IM_MyloginActivity_myshrinepicture"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:src="@drawable/toy" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="23dp"
            android:layout_height="23dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/toright" />

        <Button
            android:id="@+id/BT_MyloginActivity_myenshrine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/imageView8"
            android:layout_toRightOf="@+id/IM_MyloginActivity_myshrinepicture"
            android:background="#32A6C0"
            android:text="" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/BT_MyloginActivity_myenshrine"
            android:layout_centerVertical="true"
            android:layout_marginLeft="17dp"
            android:text="收藏" />

    </RelativeLayout>

</RelativeLayout>